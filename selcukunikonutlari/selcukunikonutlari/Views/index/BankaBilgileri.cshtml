@{
    ViewBag.Title = "BankaBilgileri";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<selcukunikonutlari.Models.BankaBilgileri>


<h2>Banka Bilgileri</h2>
<table>
    <tr>
        <td>Banka Adı</td>
        <td>İban No</td>
        <td>Hesap No</td>
        <td>Şube No</td>
    </tr>
    @if (Model!=null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>@item.bankaadi</td>
                <td>@item.iban</td>
                <td>@item.hesapno</td>
                <td>@item.subeno</td>
            </tr>
        }
    }
    else
    {
        <tr>
            Hiç bir veriye ulaşılamadı
        </tr>
    }
</table>

