@{
    ViewBag.Title = "YapilanHarcamaKaydi";
    Layout = "~/Views/Shared/_yonetim.cshtml";
}

<h2>Yapilan Harcama Kaydi</h2>

@using (Html.BeginForm("YapilanHarcamaKaydi", "Yonetim", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="forms">
        <fieldset>
            <ol>
                <li class="form-row text-area-row">
                    <label>Yapılan Harcamalar Dosyası</label>
                    <input type="file" name="exel" />
                </li>
                <li class="button-row">
                    <input type="submit" value="Gönder" name="submit" class="btn-submit" />
                </li>
            </ol>

        </fieldset>
    </div>
}